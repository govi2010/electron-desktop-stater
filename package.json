{
  "name": "electron-stater",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "npm run postinstall.web && nx serve angular-app-web-angular-app",
    "build": "npm run postinstall.web && nx build angular-app-web-angular-app",
    "test": "npm run postinstall.web && nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "npm run postinstall.web && nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "start.web.angular-app": "nx serve web-angular-app",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.electron-app": "npm run prepare.electron.electron-app && ng build electron-electron-app --prod --base-href ./",
    "build.electron.electron-app.local": "npm run build.electron.electron-app && electron dist/apps/electron-electron-app",
    "build.electron.electron-app.linux": "npm run build.electron.electron-app && cd dist/apps/electron-electron-app && npx electron-builder build --linux",
    "build.electron.electron-app.windows": "npm run build.electron.electron-app && cd dist/apps/electron-electron-app && npx electron-builder build --windows",
    "build.electron.electron-app.mac": "npm run build.electron.electron-app && cd dist/apps/electron-electron-app && npx electron-builder build --mac",
    "prepare.electron.electron-app": "npm run postinstall.electron && tsc -p apps/electron-electron-app/tsconfig.json",
    "serve.electron.electron-app.target": "nx serve electron-electron-app",
    "serve.electron.electron-app": "wait-on http-get://localhost:4200/ && electron apps/electron-electron-app/src --serve",
    "start.electron.electron-app": "npm run prepare.electron.electron-app && npm-run-all -p serve.electron.electron-app.target serve.electron.electron-app",
    "start.angular.app-web-angular-app": "npm run postinstall.web && nx serve angular-app-web-angular-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "core-js": "^3.3.2",
    "rxjs": "~6.5.3",
    "zone.js": "~0.10.2",
    "@electron-stater/scss": "file:libs/scss",
    "@ngx-translate/core": "~11.0.0",
    "@ngx-translate/http-loader": "~4.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@nrwl/angular": "~8.8.0",
    "@nrwl/workspace": "9.1.2",
    "@nstudio/angular": "9.0.0",
    "@nstudio/electron": "9.0.0",
    "@nstudio/electron-angular": "9.0.0",
    "@nstudio/web": "9.0.0",
    "@nstudio/web-angular": "9.0.0",
    "@nstudio/xplat": "^9.0.0",
    "@types/node": "~8.9.4",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.7.4",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@angular-devkit/build-angular": "0.900.1",
    "codelyzer": "~5.2.0",
    "jest-preset-angular": "7.0.0",
    "@nrwl/jest": "8.8.3",
    "jest": "24.1.0",
    "@types/jest": "24.0.9",
    "ts-jest": "24.0.0",
    "cypress": "3.4.1",
    "@nrwl/cypress": "8.8.3",
    "electron": "^7.0.1",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "~1.8.6",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~14.1.0",
    "electron-reload": "~1.5.0",
    "electron-store": "~5.1.0",
    "electron-updater": "~4.2.0",
    "npm-run-all": "^4.1.5",
    "wait-on": "~3.3.0"
  },
  "xplat": {
    "prefix": "electron-stater",
    "framework": "angular"
  }
}
